msgctxt "field:account.bank.statement.line,account_date:"
msgid "Account Date"
msgstr "Data de comptabilització"

msgctxt "field:account.bank.statement.line,account_date_utc:"
msgid "Account Date UTC"
msgstr "Data de comptabilització UTC"

msgctxt "field:account.bank.statement.line,counterpart_lines:"
msgid "Counterpart"
msgstr "Efectes"

msgctxt "field:account.move.line,bank_statement_line_counterpart:"
msgid "Bank Statement Line Counterpart"
msgstr "Contrapartida de l'extracte bancari"

msgctxt "model:ir.message,text:account_not_bank_reconcile"
msgid ""
"The of Bank Statement Journal \"%(journal)s\" is not checked as \"Bank "
"Conciliation\"."
msgstr ""
"El diari d'extractes bancaris \"%(journal)s\" no està marcat com a "
"\"conciliació bancària\"."

msgctxt "model:ir.message,text:account_statement_journal"
msgid ""
"Please provide debit and credit account on statement journal "
"\"%(journal)s\"."
msgstr ""
"Proporcioni el compte de dèbit i crèdit en el diari de l'extracte "
"\"%(journal)s\"."

msgctxt "model:ir.message,text:not_found_counterparts"
msgid ""
"Not found a counterpart account. Check accounts from journal and move lines "
"are not the same."
msgstr ""
"No s'ha trobat un compte de contrapart. Comproveu els comptes del diari i "
"les línies de moviment no són el mateix."

msgctxt "model:ir.message,text:reconciliation_cannot_delete"
msgid ""
"Cannot break reconciliation \"%(reconciliation)s\" because line \"%(line)s\""
" has assigned to Bank Statement Line Please go to \"%(statement_line)s\" "
"bank statement line to unreconcile"
msgstr ""
"No es pot trencar la conciliació \"%(reconciliation)s\" perquè la línia "
"\"%(line)s\" s'ha assignat a la línia de l'extracte bancari. Aneu a la línia"
" de l'extracte bancari \"%(statement_line)s\" per no conciliar."

msgctxt "model:ir.message,text:same_account"
msgid ""
"Account \"%(account)s\" in statement line \"%(line)s\" is the same as the "
"one configured on journal \"%(journal)s\"."
msgstr ""
"El compte \"%(account)s\" en la línia d'extracte \"%(line)s\" és el mateix "
"que el configurat al diari \"%(journal)s\"."
