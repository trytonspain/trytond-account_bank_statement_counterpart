msgctxt "field:account.bank.statement.line,account_date:"
msgid "Account Date"
msgstr "Fecha de contabilización"

msgctxt "field:account.bank.statement.line,account_date_utc:"
msgid "Account Date UTC"
msgstr "Fecha de contabilización UTC"

msgctxt "field:account.bank.statement.line,counterpart_lines:"
msgid "Counterpart"
msgstr "Efectos"

msgctxt "field:account.move.line,bank_statement_line_counterpart:"
msgid "Bank Statement Line Counterpart"
msgstr "Contrapartida del extracto bancario"

msgctxt "model:ir.message,text:account_not_bank_reconcile"
msgid ""
"The of Bank Statement Journal \"%(journal)s\" is not checked as \"Bank "
"Conciliation\"."
msgstr ""
"El diario de extractos bancarios \"%(journal)s\" no está marcado como "
"\"conciliación bancaria\"."

msgctxt "model:ir.message,text:account_statement_journal"
msgid ""
"Please provide debit and credit account on statement journal "
"\"%(journal)s\"."
msgstr ""
"Proporcione la cuenta de débito y crédito en el diario del extracto "
"\"%(journal)s\"."

msgctxt "model:ir.message,text:not_found_counterparts"
msgid ""
"Not found a counterpart account. Check accounts from journal and move lines "
"are not the same."
msgstr ""
"No se ha encontrado una cuenta de contraparte. Compruebe las cuentas del "
"diario y las líneas de movimiento no son lo mismo."

msgctxt "model:ir.message,text:reconciliation_cannot_delete"
msgid ""
"Cannot break reconciliation because there is a line assigned to Bank "
"Statement Line. Please go to \"%(bank_line)s\" bank statement line to "
"unreconcile."
msgstr ""
"No se puede romper la conciliación porque hay una línea asignada a una línea"
" de extracto bancario. Id a la línea \"%(bank_line)s\" del extracto bancario "
"para romper la conciliación."

msgctxt "model:ir.message,text:same_account"
msgid ""
"Account \"%(account)s\" in statement line \"%(line)s\" is the same as the "
"one configured on journal \"%(journal)s\"."
msgstr ""
"La cuenta \"%(account)s\" en la línea de extracto \"%(line)s\" es el mismo "
"que el configurado en el diario \"%(journal)s\"."
